<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/SCREEN"
    tools:context=".activities.Welcome_end">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="32dp"
        android:text="Welcome to your Journal"
        android:textAlignment="textStart"
        style="@style/TEXT"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnGot"
        android:layout_width="187dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="65dp"
        style="@style/BUTTON"
        android:text="Got it!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_60ssp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4">


        <TextView
            android:id="@+id/Save"
            android:layout_width="237dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            style="@style/WELCOME"
            android:text="Save moments in your life"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textSize="18sp"
            android:textStyle="bold"

            />

        <TextView
            android:id="@+id/icon1"
            android:layout_width="94dp"
            android:layout_height="match_parent"
            android:background="@drawable/icon"
            android:text="" />

        <ImageView
            android:id="@+id/icnHeart"
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="27dp"
            android:contentDescription="@string/todo"
            android:src="@drawable/book_heart"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/def1"
            android:layout_width="220dp"
            android:layout_height="42dp"
            android:text="Watch how you change and grow"
            android:textAlignment="center"
            android:layout_marginLeft="100dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="35dp"
            android:layout_marginBottom="20dp"
            style="@style/WELCOME"
            />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="336dp"
        android:layout_height="74dp"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="97dp"
            android:layout_height="match_parent"
            android:background="@drawable/icon"

            android:text="" />

        <TextView
            android:id="@+id/txt2"

            android:layout_width="240dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            style="@style/WELCOME"
            android:lineSpacingExtra="8sp"
            android:text="Keep your thoughts and ideas private"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textSize="14sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/icnLock"
            android:layout_width="40dp"

            android:layout_height="40dp"

            android:layout_marginLeft="27dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="20dp"
            android:contentDescription="@string/todo"
            android:src="@drawable/secure"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/def2"
            android:layout_width="220dp"
            android:layout_height="42dp"
            android:text="Your journal is for your eyes only"
            android:textAlignment="center"
            android:layout_marginLeft="102dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="35dp"
            android:layout_marginBottom="20dp"
            style="@style/WELCOME"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="336dp"
        android:layout_height="74dp"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="99dp"
            android:layout_height="match_parent"
            android:background="@drawable/icon"

            android:text="" />

        <TextView
            android:id="@+id/txt3"
            android:layout_width="240dp"
            android:layout_height="match_parent"

            android:layout_gravity="right"
            style="@style/WELCOME"
            android:text="Reflect your emotions"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textSize="18sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/icnGraph"
            android:layout_width="42dp"

            android:layout_height="42dp"

            android:layout_marginLeft="27dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="20dp"
            android:contentDescription="@string/todo"
            android:src="@drawable/graph"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/def3"
            android:layout_width="220dp"
            android:layout_height="42dp"
            android:text="Keep track of how you're feeling"
            android:textAlignment="center"
            android:layout_marginLeft="110dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="35dp"
            android:layout_marginBottom="20dp"
            style="@style/WELCOME"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="336dp"
        android:layout_height="74dp"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/btnGot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout3">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="97dp"
            android:layout_height="match_parent"
            android:background="@drawable/icon"

            android:text="" />

        <TextView
            android:id="@+id/txt4"
            android:layout_width="240dp"
            android:layout_height="match_parent"

            android:layout_gravity="right"
            style="@style/WELCOME"
            android:text="Play Tic Tac Toe"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textSize="18sp"
            android:textStyle="bold" />
        <ImageView
            android:id="@+id/icnTic"
            android:layout_width="42dp"

            android:layout_height="42dp"

            android:layout_marginLeft="27dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="20dp"
            android:contentDescription="@string/todo"
            android:src="@drawable/tictac"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/def4"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="20dp"
            android:text="Earn points, unlock themes, and remove ads"
            style="@style/WELCOME"
            android:textAlignment="center" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>